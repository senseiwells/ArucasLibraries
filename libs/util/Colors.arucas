interface RGBColor {
    fun getR();

    fun getG();
    
    fun getB();
    
    fun getHexCode();
}

class VarColor: RGBColor {
    var R: Number;
    var G: Number;
    var B: Number;
    var hex: Number;

    VarColor(r: Number, g: Number, b: Number) {
        this.R = r;
        this.G = g;
        this.B = b;
        this.hex = b + 256 * g + 65536 * r;
    }

    static fun fromHexCode(code: Number) {
        b = Math.mod(code, 256);
        code = (code - b) / 256;
        g = Math.mod(code, 256);
        code = (code - g) / 256;
        r = Math.mod(code, 256);
        return new VarColor(r, g, b);
    }

    fun getR() {
        return this.R;
    }

    fun getG() {
        return this.G;
    }

    fun getB() {
        return this.B;
    }

    fun getHexCode() {
        return this.hex;
    }
}

